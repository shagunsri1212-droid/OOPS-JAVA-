class ThsDemo {

    int x, y;
    //class variables
    public void get(int a, int b) {
        //x and y get func ke local variables
        a = x;
        b = y;
    }
    public void put (){
        System.out.println("a = "+x+" b = "+y);

    }
}
class Demo{
    public static void main(String[] args) {
        ThsDemo ob1 = new ThsDemo();
        ob1.get(10, 20);
        ob1.put();
    }
}
